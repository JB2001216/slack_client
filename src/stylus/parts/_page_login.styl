.columnWrap{
  &_right{
    position: relative;
    &_back{
      position: absolute;
      left: 120px;
      bottom: 81px;
      a{
        font-family: $fontFamilyBody;
        font-size: 14px;
        line-height: 1;
        color: #333333;
        text-decoration: none;
        span{
          display: inline-block;
          margin-left: 5px;
        }
      }
    }
    &_help{
      position: absolute;
      left: 100px;
      bottom: 81px;
      p{
        display: inline-block;
        font-family: $fontFamilyBody;
        font-size: 14px;
        line-height: 1;
        color: #333333;
        text-decoration: none;
      }
      a,a:visited{
        display: inline-block;
        margin-left: 5px;
        color: $themeColors.active
        text-decoration: none;
      }
      a:hover{
        text-decoration: underline;
      }
    }
  }
}
