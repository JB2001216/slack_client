  font-style: normal
  font-weight: 100
  src: url('~@/assets/fonts/noto-sans-jp/NotoSansJP-Thin.woff2') format('woff2'),
    url('~@/assets/fonts/noto-sans-jp/NotoSansJP-Thin.woff') format('woff'),
    url('~@/assets/fonts/noto-sans-jp/NotoSansJP-Thin.otf') format('opentype')

