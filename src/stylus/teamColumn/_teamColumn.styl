.teamColumn{
  color: #FFF;
  background: #1F081F;
  max-height: 100vh;
  overflow-y: auto;
  &_nav{
    margin-top: 40px;
    a{
      display: block;
      width: 40px;
      height: 40px;
      background: rgba(255, 255, 255, 0.16);
      border-radius: 8px;
      margin: 0 10px 20px;
      transition: 0.5s;
      &:hover{
        background: #EDEDED;
      }
      &.active{
        background: #EDEDED;
      }
      &.teamColumn_add{
        display: flex;
        justify-content: center;
        align-items: center;
        box-sizing: border-box;
        background: transparent;
        fill: #EDEDED;
        border: 1px solid #EDEDED;
        &:hover{
          fill: #1F081F;
          background: #EDEDED;
        }
      }
    }
  }
}